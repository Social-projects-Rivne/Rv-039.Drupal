
{%
  set classes = [
    'paragraph',
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
    not paragraph.isPublished() ? 'paragraph--unpublished'
  ]
%}
<section  class="page_software_your_airspace"  style="background:url({{ background_image_url }}) no-repeat center top; background-position: 0 50%;background-size: cover;" >

  {% block paragraph %}
    <div{{ attributes.addClass(classes) }}>
      {% block content %}
        {{ content.field_label_your_airspace_secure }}
      {% endblock %}
    </div>
  {% endblock paragraph %}
</section>
